{
  "name": "melthedev/magento-disable-search-engine",
  "version": "1.0.2",
  "description": "Module used to disable Elastic and OpenSearch",
  "type": "magento2-module",
  "keywords": ["magento","search-engine", "disable-search"],
  "require": {
    "php": "^8.0|^8.1",
    "magento/framework": "103.0.*",
    "magento/module-catalog": "104.0.*",
    "magento/module-indexer": "100.4.*",
    "magento/module-search": "101.1.*",
    "magento/module-catalog-search": "102.0.*"
  },
  "license": [
    "MIT"
  ],
  "authors": [
    {
      "name": "MelTheDev",
      "email": "mel@melthedev.com"
    }
  ],
  "autoload": {
    "files": [
      "registration.php"
    ],
    "psr-4": {
      "MelTheDev\\DisableSearchEngine\\": ""
    }
  },
  "repositories": {
    "repo.magento.com": {
      "type": "composer",
      "url": "https://repo.magento.com/"
    }
  }
}
